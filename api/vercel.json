{
  "version": 2,
  "builds": [
    {
      "src": "api/service.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/gigs/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/orders/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/conversations/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/messages/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/api/reviews/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/service.js"
    },
    {
      "src": "/",
      "dest": "/api/public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/api/public/$1"
    }
  ]
}
