{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/gigs/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/conversations/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/messages/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/reviews/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/(.*)",
      "dest": "service.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ],

  "rewrites": [{ "source": "/(.*)", "destination": "server.js" }]
}
